<h1>Edit Application</h1>

<%= form_for [@user, @registered_application] do |f| %>
<div class="form-group">
  <%= f.label :appname %>
  <%= f.text_field :appname, class: 'form-control', placeholder: "Enter site's name here" %>
  <%= f.label :url %>
  <%= f.text_field :url, class: 'form-control', placeholder: "Enter site's URL here" %>
</div>
<div class="form-group">
  <%= f.submit "Save", class: 'btn btn-success' %>
</div>
<% end %>
